{
  "name": "sandbooks-backend",
  "version": "1.0.0",
  "description": "Backend API for Sandbooks - Node.js Express server with Hopx SDK for sandboxed code execution",
  "main": "dist/index.js",
  "type": "commonjs",
  "keywords": [
    "sandbooks",
    "code-execution",
    "express",
    "hopx",
    "api",
    "backend"
  ],
  "homepage": "https://sandbooks.space",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahutanu/sandbooks.space.git",
    "directory": "backend"
  },
  "bugs": {
    "url": "https://github.com/ahutanu/sandbooks.space/issues"
  },
  "author": {
    "name": "Alex Hutanu",
    "email": "alex@hutanu.net",
    "url": "https://hutanu.net"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "predev": "node -e \"const fs=require('fs');const path=require('path');const env=path.join(__dirname,'.env');const example=path.join(__dirname,'.env.example');if(!fs.existsSync(env)&&fs.existsSync(example)){fs.copyFileSync(example,env);}\"",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hopx-ai/sdk": "^0.3.4",
    "@types/ws": "^8.18.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "highlight.js": "^11.11.1",
    "node-pty": "^1.0.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.56.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/node": "^20.10.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.12",
    "@vitest/ui": "^4.0.12",
    "msw": "^2.12.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.12"
  }
}
